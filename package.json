{
  "name": "radio-rasclat",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot",
    "serve:sw": "npm run build -s && npx http-server ./dist -p 4200",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test --code-coverage --watch=false",
    "test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s && opencollective-postinstall",
    "generate": "ng generate"
  },
  "collective": {
    "url": "https://opencollective.com/radio-rasclat"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@ngx-translate/core": "^11.0.1",
    "@sentry/browser": "^5.6.3",
    "@sentry/cli": "^1.49.0",
    "@sentry/integrations": "^5.11.1",
    "@types/algoliasearch-helper": "^2.26.1",
    "@types/jquery": "^3.3.31",
    "algoliasearch": "^3.34.0",
    "angular-feather": "^6.0.2",
    "angular-instantsearch": "^3.0.0-beta.4",
    "angulartics2": "7.5.2",
    "bootstrap": "^4.1.1",
    "codecov": "^3.6.1",
    "core-js": "^2.6.3",
    "flashgrid": "^1.1.1",
    "instantsearch.css": "^7.3.1",
    "instantsearch.js": "^3.6.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "ng-lazyload-image": "^7.0.1",
    "ng2-tooltip-directive": "^2.9.20",
    "ngx-swiper-wrapper": "^8.0.2",
    "ngx-timeago": "^1.0.3",
    "node-vibrant": "^3.1.4",
    "opencollective-postinstall": "^2.0.2",
    "popper.js": "^1.15.0",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.27",
    "@angular/cli": "~8.3.27",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^3.0.4",
    "@types/algoliasearch": "^3.30.18",
    "@types/jasmine": "^3.4.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^8.10.54",
    "codelyzer": "^5.0.1",
    "hads": "^2.0.1",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^2.2.4",
    "husky": "^3.0.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-junit-reporter": "^1.2.0",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.0",
    "puppeteer": "^1.19.0",
    "stylelint": "~10.1.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended-scss": "~3.3.0",
    "stylelint-config-standard": "~18.3.0",
    "stylelint-scss": "~3.10.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.19.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.5.3"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
